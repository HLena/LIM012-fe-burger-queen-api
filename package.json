{
  "name": "burger-queen-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "babel src/index.js --out-dir dist",
    "dev": "nodemon src/index.js --exec babel-node",
    "start": "node src/index.js",
    "pretest": "eslint .",
    "test:e2e": "jest --verbose --testPathPattern e2e --globalSetup ./e2e/globalSetup --globalTeardown ./e2e/globalTeardown --runInBand",
    "test:unit": "jest --verbose --testPathIgnorePatterns e2e",
    "test": "npm run test:e2e && npm run test:unit",
    "lint": "eslint .",
    "docs:generate": "jsdoc -c jsdoc.conf --readme README-docs.md",
    "docs:deploy": "rm -rf docs && npm run docs:generate && gh-pages -d docs"
  },
  "dependencies": {
    "@babel/polyfill": "7.10.1",
    "bcrypt": "3.0.8",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "mongodb": "3.5.9",
    "mongoose": "5.9.20"
  },
  "devDependencies": {
    "@babel/cli": "7.10.3",
    "@babel/core": "7.10.3",
    "@babel/node": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "docdash": "1.1.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "gh-pages": "2.1.1",
    "jest": "24.9.0",
    "jsdoc": "3.6.3",
    "jsdoc-http-plugin": "0.3.1",
    "node-fetch": "2.6.0",
    "nodemon": "2.0.4",
    "tree-kill": "1.2.1"
  }
}
